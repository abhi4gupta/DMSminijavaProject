<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="Anch" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Signup">
   <children>
      <TextField fx:id="fn" layoutX="25.0" layoutY="48.0" promptText="First Name" />
      <TextField fx:id="ln" layoutX="25.0" layoutY="87.0" promptText="Last Name" />
      <TextField fx:id="us" layoutX="25.0" layoutY="132.0" promptText="Username" />
      <TextField fx:id="cp" layoutX="25.0" layoutY="174.0" promptText="Create Password" />
      <TextField fx:id="cfp" layoutX="25.0" layoutY="216.0" promptText="Confirm Password" />
      <Button fx:id="su" layoutX="74.0" layoutY="269.0" mnemonicParsing="false" onAction="#buttonsin" text="Sign Up" />
      <Button fx:id="ok" layoutX="148.0" layoutY="269.0" mnemonicParsing="false" onAction="#buttonclo" text="ok" />
   </children>
</AnchorPane>
